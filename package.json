{
  "name": "knockout-store",
  "version": "1.0.0",
  "description": "State management for Knockout apps.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-spreetail": "^1.0.3",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-preprocessor": "^4.0.0",
    "knockout": "^3.4.2",
    "mocha": "^3.3.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2"
  },
  "peerDependencies": {
    "knockout": "*"
  },
  "scripts": {
    "test": "karma start",
    "test-once": "karma start --single-run",
    "test-with-chrome": "karma start --browsers Chrome",
    "lint": "eslint src",
    "build:cjs": "rollup --config rollup/rollup.cjs.config.js",
    "build:umd": "rollup --config rollup/rollup.umd.config.js",
    "build:umd:min": "rollup --config rollup/rollup.umd.min.config.js",
    "build": "npm run build:cjs && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist",
    "build-fresh": "npm run clean && npm run build",
    "preversion": "npm run lint && npm run test-once && npm run build-fresh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spreetail/knockout-store.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "knockout",
    "state",
    "store",
    "connect"
  ],
  "author": "Alex Bainter <alex.bainter@spreetail.com> (https://github.com/metalex9)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Spreetail/knockout-store/issues"
  },
  "homepage": "https://github.com/Spreetail/knockout-store#readme"
}
